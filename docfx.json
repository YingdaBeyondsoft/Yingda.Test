{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**"
        ],
        "src": "articles",
        "dest": "."
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**"
        ],
        "src": "bread",
        "dest": "bread"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": "articles",
        "dest": "."
      },
      {
        "files": [
          "includes/**/*.svg",
          "includes/**/*.png",
          "includes/**/*.jpg",
          "includes/**/*.jpeg",
          "includes/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "bilingual_type": "hover over"
    },
    "fileMetadata": {},
    "template": [
      "docs.html"
    ]
  }
}